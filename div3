import java.util.*;
class divisible{
    public static void main(String[] args) {
        System.out.print("Enter a number:");
        Scanner k=new Scanner(System.in);
        int a= k.nextInt();
        int b=a%10;
        if (b%3==0){
            System.out.println("It is divisible by 3");
        }else{
            System.out.println("It is not divisible by 3 ");
        }
        k.close();
        
    }}
