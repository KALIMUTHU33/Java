import java.lang.*;
import java.util.*;
class char1{
    public static void main(String[] args) {
        
        System.out.print("Enter a character for the ascci value:");
        Scanner k=new Scanner(System.in);
        char n;
        n=k.next().charAt(0);
        int n1=(int)n;
        System.out.print("The ascci value of the given character is:");
        System.out.print(n1);
        k.close();


    }
}
